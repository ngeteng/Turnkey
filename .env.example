# Isi dengan semua Private Key Anda, mulai dari PRIVATE_KEY_1, PRIVATE_KEY_2, dst.
PRIVATE_KEY_1=0x...
PRIVATE_KEY_2=0x...

# Konfigurasi Jaringan (defaultnya Sepolia, bisa diubah)
RPC_URL=https://ethereum-sepolia-rpc.publicnode.com
CHAIN_ID=11155111
EXPLORER_URL=https://sepolia.etherscan.io

# Konfigurasi Transaksi
# Jumlah transaksi yang akan dikirim per wallet
TX_COUNT=3

# Rentang jumlah ETH yang akan dikirim (acak di antara nilai ini)
MIN_AMOUNT=0.000001
MAX_AMOUNT=0.0000022

# Jeda antar transaksi (dalam milidetik)
# Contoh: 2000-5000 artinya jeda acak antara 2 sampai 5 detik
MIN_TX_DELAY=2000
MAX_TX_DELAY=5000

# Jeda antar wallet (setelah semua tx di satu wallet selesai)
MIN_WALLET_DELAY=5000
MAX_WALLET_DELAY=10000
